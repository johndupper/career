<!-- search results page -->

<% @all_jobs.each do |job| %>

      Job Title:
      <input type="text" name="title" value="<%= job['jobtitle'] %>"><br>
      Company:
      <input type="text" name="company" value="<%= job['company'] %>"><br>
      Location:
      <input type="text" name="location" value="<%= job['formattedLocation'] %>"><br>
      Details:
      <input type="text" name="snippet" value="<%= job['snippet'] %>"><br>
      Posting Date:
      <input type="text" name="date" value="<%= job['date'] %>"><br>
      <input type="submit" value="Submit"><br>

<% end %>